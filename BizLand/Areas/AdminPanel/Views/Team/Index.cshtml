@using BizLand.Models
@model IEnumerable<TeamMember>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/AdminPanel/Views/Shared/_AdminLayout.cshtml";
}
<div class="all-content-wrapper">
    <div class="product-status mg-b-30">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="product-status-wrap">
                        <h4>Member List</h4>
                        <div class="add-product">
                            <a href="product-edit.html">Add Member</a>
                        </div>
                        <table>
                            <tr>
                                <th>Image</th>
                                <th>Name</th>
                                <th>SurName</th>
                                <th>JobName</th>
                                <th>TwitterURl</th>
                                <th>LinkedinURl</th>
                                <th>FaceBookURl</th>
                                <th>InstagramURl</th>
                            </tr>

                            @foreach (var member in Model)
                            {
                                <tr>
                                    <td><img style="width:15    0px ;border-radius:15px" src="~/img/@member.URl" alt="" /></td>
                                    <td>@member.Name</td>
                                    <td>@member.SurName</td>
                                    <td>@member.JobName</td>
                                    <td>@member.TwitterURl</td>
                                    <td>@member.LinkedinURl</td>
                                    <td>@member.FaceBookURl</td>
                                    <td>@member.InstagramURl</td>
                                    <td>
                                        <a data-toggle="tooltip" asp-action="Update" asp-route-id="@member.Id" title="Edit" class="pd-setting-ed"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>
                                        <a data-toggle="tooltip" asp-action="Delete" asp-route-id="@member.Id" title="Trash" class="pd-setting-ed"><i class="fa fa-trash-o" aria-hidden="true"></i></a>
                                    </td>


                                </tr>
                            }
                        </table>
                        <div class="custom-pagination">
                            <ul class="pagination">
                                <li class="page-item"><a class="page-link" href="#">Previous</a></li>
                                <li class="page-item"><a class="page-link" href="#">1</a></li>
                                <li class="page-item"><a class="page-link" href="#">2</a></li>
                                <li class="page-item"><a class="page-link" href="#">3</a></li>
                                <li class="page-item"><a class="page-link" href="#">Next</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer-copyright-area">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="footer-copy-right">
                        <p>Copyright © 2018 <a href="https://colorlib.com/wp/templates/">Colorlib</a> All rights reserved.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

